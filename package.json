    "ethers": "^6.11.1",
    "express": "^4.18.3",
    "gl": "^6.0.2",
    "knex": "^3.1.0",
    "load-json-file": "^7.0.1",
    "pg": "^8.11.3",
    "pngjs": "^7.0.0",
    "pretty-seconds": "^3.0.1",
    "text-to-image": "^5.2.0",
    "three": "^0.162.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "nodemon": "^3.1.0"
